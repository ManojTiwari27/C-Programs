#include<stdio.h>
void main()
{
    int year;
    printf("Enter a year that you want to check leap year or not\n");
    scanf("%d",&year);
    if(year%100==0 )
    {
        if(year%400==0)
            printf("Leap year");
        else
            printf("Non leap Year");
    }
    else if (year%4 == 0)
        printf("Leap year");
    else
        printf("Non leap year");
}
