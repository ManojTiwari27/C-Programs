#include<stdio.h>
void main()
{
    int l,u,i,j;
    printf("Enter Starting point and Ending point to check prime numbers ");
    scanf("%d\n%d",&l,&u);
    for(i=l;i<=u;i++)
    {
        for(j=2;j<i;j++)
        {
        if(i%j==0)
            break;
        }
        if(j==i)
            printf("%d\n",j);

    }

}
